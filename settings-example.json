{
  "default_endpoint": "https://localhost:5003",
  "endpoints": [
    {
      "method": "GET",
      "path": "/api/v1/endpoint/{id}",
      "status": 200,
      "content_type": "application/json",
      "payload": {
        "users": [
          { "id": 1, "name": "Alice" },
          { "id": 2, "name": "Bob" }
        ]
      }
    },
    {
      "method": "GET",
      "path": "/welcome/{username}",
      "status": 200,
      "content_type": "text/html",
      "payload": "<html><body><h1>Welcome, {{username}}!</h1><p>We're glad to have you here, {{username}}.</p></body></html>"
    }
  ]
}
